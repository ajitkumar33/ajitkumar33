# slicing 
a = "python is a good programming language to learn"
print(a[1:26])
print(a[::47])
print(a[-1::])
print(a[2:36])
print(a[1:-18])
print(a[-30:-1])
print(a[0::])
print(a[-2::])
print(a[2::7])
print(a[-2::-5])
print(j[-40:-4:10])
print(a[2:-2:3])
print(a[:-1])
print(a[-30:-1:2])
print(a[10::])
